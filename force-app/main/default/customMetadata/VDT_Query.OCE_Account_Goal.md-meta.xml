<?xml version="1.0" encoding="UTF-8"?>
<CustomMetadata xmlns="http://soap.sforce.com/2006/04/metadata" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
    <label>OCE Account Goal</label>
    <protected>false</protected>
    <values>
        <field>Module_Name__c</field>
        <value xsi:type="xsd:string">Master Data Analysis</value>
    </values>
    <values>
        <field>Name__c</field>
        <value xsi:type="xsd:string">Plan_Targets_By_Account</value>
    </values>
    <values>
        <field>Package_Version__c</field>
        <value xsi:type="xsd:string">OCE</value>
    </values>
    <values>
        <field>Query__c</field>
        <value xsi:type="xsd:string">SELECT OCE__Account__c accountId, COUNT(Id) counter
FROM OCE__AccountGoal__c
WHERE OCE__Account__c IN :accountIds
AND OCE__PlanCycle__r.OCE__StartDate__c &gt;= :startDate AND OCE__PlanCycle__r.OCE__EndDate__c &lt;= :endDate
GROUP BY OCE__Account__c</value>
    </values>
</CustomMetadata>
